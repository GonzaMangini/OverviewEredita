{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/**/index.go", "use": "@vercel/go" }
  ],
  "rewrites": [
    { "source": "/api/v1/products/:id", "destination": "/api/v1/products/_id" },
    { "source": "/(.*)", "destination": "/$1" }
  ]
}